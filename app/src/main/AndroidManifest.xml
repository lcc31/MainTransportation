<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maintransportation">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MainTransportation">
        <activity android:name=".AboutActivity"></activity>
        <activity android:name=".database.ChartActivity" />
        <activity android:name=".database.ProfileActivity" />
        <activity android:name=".database.AddDBClientActivity" />
        <activity android:name=".database.DBActivity" />
        <activity android:name=".AddClientActivity" />
        <activity android:name=".orar.BucurestiActivity" />
        <activity android:name=".orar.GiurgiuActivity" />
        <activity android:name=".RezervareActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>